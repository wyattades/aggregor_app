{
  "name": "Aggregor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "rm -rf ./node_modules/redux-form/node_modules/react && rm -rf ./node_modules/react-native-timeago/node_modules/react",
    "test": "jest",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "emulator": "$ANDROID_HOME/emulator/emulator -avd nexus",
    "debug": "~/Documents/RNDebugger/React_Native_Debugger",
    "build": "(cd android/ && ./gradlew assembleDebug)",
    "start": "adb reverse tcp:3000 tcp:3000; adb reverse tcp:8081 tcp:8081; node node_modules/react-native/local-cli/cli.js start",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/"
  },
  "remotedev": {
    "hostname": "localhost",
    "port": 5678
  },
  "rnpm": {
    "assets": ["./assets/fonts"]
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-material-ui": "^1.12.0",
    "react-native-prompt": "^1.0.0",
    "react-native-vector-icons": "^4.1.1",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-thunk": "^2.2.0",
    "react-native-timeago": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-native": "^2.3.2",
    "immutable-devtools": "0.0.7",
    "jest": "19.0.2",
    "react-native-dotenv": "0.0.4",
    "react-test-renderer": "16.0.0-alpha.6",
    "redux-persist": "^4.7.1",
    "remote-redux-devtools": "^0.5.7"
  },
  "jest": {
    "preset": "react-native"
  }
}
