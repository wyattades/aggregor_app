{
  "name": "aggregor",
  "version": "0.0.1",
  "scripts": {
    "test": "jest",
    "lint": "node node_modules/eslint/bin/eslint.js ./src",
    "android": "adb uninstall \"com.aggregor\"; node node_modules/react-native/local-cli/cli.js run-android",
    "android-prod": "adb uninstall \"com.aggregor\"; cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "emulator": "$ANDROID_HOME/emulator/emulator -avd nexus",
    "debug": "~/Documents/RNDebugger/React_Native_Debugger",
    "start": "adb reverse tcp:3000 tcp:3000; node node_modules/react-native/local-cli/cli.js start",
    "build": "export NODE_ENV=production && node ./node_modules/webpack/bin/webpack.js --progress --profile --colors",
    "dev": "export NODE_ENV=development && node server.js",
    "deploy": "aws s3 sync web s3://aggregor.com/ --delete && cf-invalidate --wait -- E2QMYOILA6VFSB index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/aggregor_app"
  },
  "license": "MIT",
  "dependencies": {
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-infinite": "^0.12.1",
    "react-motion-drawer": "^2.1.5",
    "react-native": "0.48.0",
    "react-native-config": "^0.6.1",
    "react-native-drawer": "^2.5.0",
    "react-native-vector-icons": "^4.4.0",
    "react-native-web": "^0.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "react-sidebar": "^2.3.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-persist": "^4.10.1",
    "redux-thunk": "^2.2.0",
    "timeago.js": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "immutable-devtools": "^0.0.7",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prettier-eslint": "^8.2.0",
    "react-test-renderer": "^16.0.0",
    "remote-redux-devtools": "0.5.12",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "babel": {
    "presets": [
      "react-native"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ],
    "sourceMaps": true
  }
}
